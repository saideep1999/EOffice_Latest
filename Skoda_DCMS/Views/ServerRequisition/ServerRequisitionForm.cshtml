    function allowAlphaNumericSpace(thisInput) {
        var arr = thisInput.value.split('');

        if (arr[0] === "." || arr[0] === "-") {
            thisInput.value = "";
        } else {
            for (var i = 0; i < arr.length; i++) {
                if (arr[i] == "-" || arr[i] == "*" || arr[i] == "+" || arr[i] == "/") {
                    thisInput.value = "";
                }
            }
        }

        //thisInput.value = thisInput.value.split(/[^e]/).join('');
    }


    function GetTotalCalculation() {
        var CurrentCpu = $("#txtCurCpu").val();
        var IncrementCpu = $("#txtIncCpu").val();
        var CurrentMemory = $("#txtCurMemory").val();
        var IncrementMemory = $("#txtIncMemory").val();
        var CurrentDisk = $("#txtCurDisk").val();
        var IncrementDisk = $("#txtIncDisk").val();
        var CurrentLan = $("#txtCurLan").val();
        var IncrementLan = $("#txtIncLan").val();

        CurrentCpu = parseInt(CurrentCpu);
        IncrementCpu = parseInt(IncrementCpu);
        CurrentMemory = parseInt(CurrentMemory);
        IncrementMemory = parseInt(IncrementMemory);
        CurrentDisk = parseInt(CurrentDisk);
        IncrementDisk = parseInt(IncrementDisk);
        CurrentLan = parseInt(CurrentLan);
        IncrementLan = parseInt(IncrementLan);

        var TotCpu = CurrentCpu + IncrementCpu;
        var TotMemory = CurrentMemory + IncrementMemory;
        var TotDisk = CurrentDisk + IncrementDisk;
        var TotLAN = CurrentLan + IncrementLan;

       TotCpu= parseInt(TotCpu)
       TotMemory = parseInt(TotMemory)
       TotDisk = parseInt(TotDisk)
       TotLAN = parseInt(TotLAN)

        $("#txtTotalCpu").val(TotCpu);
        $("#txtTotalMemory").val(TotMemory);
        $("#txtTotalDisk").val(TotDisk);
        $("#txtTotalLan").val(TotLAN);


    }

    $("#txtCurOwner").autocomplete({
        source: function (request, response) {
            $.ajax({
                url: "/List/GetEmployeeDetails",
                dataType: "json",
                cache: false,
                async: false,
                data: {
                    searchText: $("#txtCurOwner").val()
                },
                success: function (data) {
                      data = data.filter(function (e) {
                        return e.Email !== "@user.Email";
                    });
                    response($.map(data, function (item) {
                        console.log(data, 'data');
                        return { label: item.FirstName, value: item.FullName, raw: item };
                    }));
                }
            });
        },
        search: function (event, ui) {
            var value = $("#txtCurOwner").val();
            // If less than three chars, cancel the search event
            if (value.length < 4) {
                event.preventDefault();
            }
        },
        min_length: 4,
        delay: 0,
        select: function (event, ui) {
            var empOtherDetails = ui.item['raw'];
            //Get the other details like Cost Center Code, Employee Number from DB.
            if (!empOtherDetails.Email) {
                document.getElementById('txtCurOwner').value = '';
                DisplayAlert('success', 'Info', 'Please select a user with valid Email', 4000);
                event.preventDefault();
                return;
            }

        }
    });

    $("#txtCurOwner").autocomplete("option", "appendTo", ".autocomplete-div");

