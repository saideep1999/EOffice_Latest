    function saveResponse(text) {
        
        var status = '', comment = '';
        comment = $("#comment-" + text).val();
        switch (text) {
            case 'approve': {
                status = 'Approved';
                if (currentUserDesgn == grcUser) {
                    if (GAIF.RequestType == "Consultation") {
                        var txtAnswers = document.getElementById("txtAnswers").value;
                        if (txtAnswers == "") {
                            AddError("txtAnswers", '', errorCount);
                            DisplayAlert('error', 'Error', 'Please enter Answers', 5000);
                            $("#collapseFive").addClass('show');
                            return;
                        }
                        else {
                            RemoveError("txtAnswers");
                            UpdateGAIFData(status, appRowId, comment, approvalType, text);
                        }
                    }
                    else {
                        if ($('.chkGrcClass:checkbox:checked').length == 0) {
                            $("#chkGrcYes").addClass('removechkGrcClass');
                            $("#chkGrcNo").addClass('removechkGrcClass');
                            AddError("lblGiftTobeDepositedWithGRC", 'removechkGrcClass', errorCount);
                            DisplayAlert('error', 'Error', 'Please check the Gift to be Deposited with GRC', 5000);
                            $("#collapseFive").addClass('show');
                            return;
                        }
                        else {
                            RemoveError("lblGiftTobeDepositedWithGRC");
                            UpdateGAIFData(status, appRowId, comment, approvalType, text);
                        }
                    }
                }
                else {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                break;
            }
            case 'reject': {
                status = 'Rejected';
                if (comment == '') {
                    validityModal(text);
                    return;
                }
                else {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                break;
            }
            case 'enquire': {
                status = 'Enquired';
                if (comment == '') {
                    validityModal(text);
                    return;
                }
                else {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                break;
            }
        }

        $("#tab").val(1);
    }

    function FormApproval(status, appRowId, comment, approvalType, text) {
        //
        spinner.show();
        $.ajax({
            url: '/List/SaveResponse',
            type: 'POST',
            data: { response: status, appRowId: appRowId, comment: comment, approvalType: approvalType },
            dataType: 'JSON',
            success: (function (data) {
                if (data.Result == 1) {
                    successGAIFFModal(text);
                    spinner.hide();
                }
                else {
                    failureModal();
                    spinner.hide();
                }
            }),
            error: (function (data) {
                failureModal();
                spinner.hide();
            })
        })

        $("#tab").val(1);
    }

    function UpdateGAIFData(status, appRowId, comment, approvalType, text) {
        //
        var form = $("#GAIForm").serialize();
        console.log(form);
        $.ajax({
            url: '/GiftsInvitation/UpdateGAIFData',
            type: 'POST',
            data: form,
            dataType: 'JSON',
            success: (function (data) {
                if (data == 1) {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                else {
                    failureModal();
                }
            }),
            error: (function (data) {
                failureModal();
            })
        })

    }

