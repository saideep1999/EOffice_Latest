        spinner.show();
        $.ajax({
        url: '/List/SaveResponse',
        type: 'POST',
        data: { response: status, appRowId: appRowId, comment: comment, approvalType: approvalType },
        dataType: 'JSON',
        success: (function (data) {
        if (data.Result == 1) {
        successQMCRModal(text);
        spinner.hide();
        //setTimeout(FilterMyTasks, 5000);
        spinner.hide();
        }
        else {
        failureModal();
        spinner.hide();
        }
        }),
        error: (function (data) {
        failureModal();
        spinner.hide();
        })
        })

        $("#tab").val(1);
        }

