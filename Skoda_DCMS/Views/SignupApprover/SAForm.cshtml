            $(".full-screen-container").hide();
            $("#txtRejectReason").addClass('errorIconTextarea');
            $("#txtRejectReason").focus()
            EnableButtons();
            return false
        }
        else {
            $("#txtRejectReason").removeClass('errorIconTextarea');
            $('#ModelReason').modal('hide');
        }

        var txtEmployeeFirstNameSU = $('#txtEmployeeFirstNameSU').val();
        var txtEmployeeMiddleNameSU = $('#txtEmployeeMiddleNameSU').val();
        var txtEmployeeLastNameSU = $('#txtEmployeeLastNameSU').val();
        var txtCostCenterSU = $('#txtCostCenterSU').val();
