        var txtDepartmentSU = $('#txtDepartmentSU').val();
        var txtSubDepartmentSU = $('#txtSubDepartmentSU').val();
        var txtManagerEmployeeCodeSU = $('#txtManagerEmployeeCodeSU').val();

        $.ajax({
            url: '/SignupApprover/SaveApproveData',
            type: 'POST',
            data: { Id: SUId, FirstName: txtEmployeeFirstNameSU, MiddleName: txtEmployeeMiddleNameSU, LastName: txtEmployeeLastNameSU, EmailID: txtEmailIdSU, PhoneNumber: txtContactNoSU, Department: txtDepartmentSU, SubDepartment: txtSubDepartmentSU, CostCenter: txtCostCenterSU, ManagerEmployeeNumber: txtManagerEmployeeCodeSU, Action: Text, RejectReason: txtRejectReason },
            dataType: 'JSON',
            success: (function (data) {

                if (data.Status == 200) {
                    $('#SignUpForm').modal('hide');
                    if (Text == "Approve")
                        successModal("success");
                    else
                        successModal("reject");
                }
                else {
                    failureModal(data.Message);
                    spinner.hide();
                }
                EnableButtons();
                $(".full-screen-container").hide();
            }),
            error: (function (data) {
                failureModal(data.Message);
                spinner.hide();
                EnableButtons();
                $(".full-screen-container").hide();
            })
        })
    }
