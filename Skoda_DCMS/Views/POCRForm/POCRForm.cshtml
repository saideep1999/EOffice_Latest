@*Calculation Part*@

<script>
    function LessStdDis(Id) {
        var ListPrice = POCRItemM[POCRItemM.findIndex(x => x.ProductId == Id)].ListPrice;
        var LessStandardDiscount = $("#LessStdDis" + Id + "").val();
        var PriceAfterStdDisc = ((ListPrice * LessStandardDiscount) - ListPrice);
        if (PriceAfterStdDisc < 0) {
            PriceAfterStdDisc = 0;
        }
        $("#LessPriceAftStdDis" + Id + "").val(PriceAfterStdDisc.toFixed(2));
    }

    function LessAddDis(Id) {
        var PriceAfterStdDisc = $("#LessPriceAftStdDis" + Id + "").val();
        var LessAdditionalDiscount = $("#LessAddDis" + Id + "").val();
        var PriceAfterAddDisc = ((PriceAfterStdDisc * LessAdditionalDiscount) - PriceAfterStdDisc);
        if (PriceAfterAddDisc < 0) {
            PriceAfterAddDisc = 0;
        }
        $("#PriceAftAddDis" + Id + "").val(PriceAfterAddDisc.toFixed(2));
    }
    function OTLSchemeDis(Id) {
        var PriceAfterAddDisc = $("#LessAddDis" + Id + "").val();
        var InBillOTLSchemeDiscount = $("#OTLSchemeDis" + Id + "").val();
        var PriceAfterinBillOTLDisc = ((PriceAfterAddDisc * InBillOTLSchemeDiscount) - PriceAfterAddDisc);
        if (PriceAfterinBillOTLDisc < 0) {
            PriceAfterinBillOTLDisc = 0;
        }
        $("#PriceAftOTLSDis" + Id + "").val(PriceAfterinBillOTLDisc.toFixed(2));
    }
    function TruckScheme(Id) {
        var PriceAfterinBillOTLDisc = $("#PriceAftOTLSDis" + Id + "").val();
        var InBillSchemeTruckScheme = $("#OTLSchemeDis" + Id + "").val();
        var PriceAfterTruckScheme = ((PriceAfterinBillOTLDisc * InBillSchemeTruckScheme) - PriceAfterinBillOTLDisc);
        if (PriceAfterTruckScheme < 0) {
            PriceAfterTruckScheme = 0;
        }
        $("#PriceAftTruckSch" + Id + "").val(PriceAfterTruckScheme.toFixed(2));
    }
    function DealerSpecific(Id) {
        var PriceAfterTruckScheme = $("#PriceAftTruckSch" + Id + "").val();
        var DealerSpecificInBillAnnual = $("#DealerSpecific " + Id + "").val();
        var PriceAfterDealerSpecificInBillAnnual = ((PriceAfterTruckScheme * DealerSpecificInBillAnnual) - PriceAfterTruckScheme);
        if (PriceAfterDealerSpecificInBillAnnual < 0) {
            PriceAfterDealerSpecificInBillAnnual = 0;
        }
        $("#PriceDealerSpecific" + Id + "").val(PriceAfterDealerSpecificInBillAnnual.toFixed(2));
    }
    function GST(Id) {
        var PriceAfterDealerSpecificInBillAnnual = $("#PriceDealerSpecific" + Id + "").val();
        var ADDGST = $("#GST" + Id + "").val();
        var ActualGSTPrice = (PriceAfterDealerSpecificInBillAnnual * ADDGST);
        if (ActualGSTPrice < 0) {
            ActualGSTPrice = 0;
        }
        $("#GSTPrice" + Id + "").val(ActualGSTPrice.toFixed(2));

    }
    function PrimaryFreight(Id) {
        var PriceAfterDealerSpecificInBillAnnual = $("#PriceDealerSpecific" + Id + "").val();
        var AddPrimaryProportionateFreight = $("#PrimaryFreight" + Id + "").val();
        var PriceAfterPropotinatePrimaryFreight = (PriceAfterDealerSpecificInBillAnnual + (PriceAfterDealerSpecificInBillAnnual * AddPrimaryProportionateFreight));
        if (PriceAfterPropotinatePrimaryFreight < 0) {
            PriceAfterPropotinatePrimaryFreight = 0;
        }
        $("#PricePriFreight" + Id + "").val(PriceAfterPropotinatePrimaryFreight.toFixed(2));
    }
    function SecondaryFreight(Id) {
        var PriceAfterPropotinatePrimaryFreight = $("#PricePriFreight" + Id + "").val();
        var AddSecondaryFreight = $("#SecondaryFreight" + Id + "").val();
        var PriceAfterAddSecdFreight = (PriceAfterPropotinatePrimaryFreight + (PriceAfterPropotinatePrimaryFreight * AddSecondaryFreight));
        if (PriceAfterAddSecdFreight < 0) {
            PriceAfterAddSecdFreight = 0;
        }
        $("#PriceSecFreight" + Id + "").val(PriceAfterAddSecdFreight.toFixed(2));
    }
    function COIForCredit(Id) {
        var PriceAfterAddSecdFreight = $("#PriceSecFreight" + Id + "").val();
        var AddCostOfInterest = $("#COIForCredit" + Id + "").val();


        var PriceAfterCostofIntPercentageForCredit = (PriceAfterAddSecdFreight + (PriceAfterAddSecdFreight * AddCostOfInterest));
        if (PriceAfterCostofIntPercentageForCredit < 0) {
            PriceAfterCostofIntPercentageForCredit = 0;
        }
        $("#PriceCOIForCredit" + Id + "").val(PriceAfterCostofIntPercentageForCredit.toFixed(2));
    }
    function DealerMargin(Id) {
        var DealerLanding = $("#DealerLanding" + Id + "").val();
        var DealerMargin = $("#DealerMargin" + Id + "").val();
        var BillingRateToTheBuilder = (DealerLanding + (DealerLanding * DealerMargin));
        if (BillingRateToTheBuilder < 0) {
            BillingRateToTheBuilder = 0;
        }
        $("#BillRateBuild" + Id + "").val(BillingRateToTheBuilder.toFixed(2));
    }
    function DealerLanding(Id) {
        var BillingRateToTheBuilder = $("#BillRateBuild" + Id + "").val();
        var DealerLanding = $("#DealerLanding" + Id + "").val();
        var DifferenceBetweenBillingRateToTheBuilderAndDealerLanding = ((BillingRateToTheBuilder - DealerLanding) / DealerLanding);
        if (DifferenceBetweenBillingRateToTheBuilderAndDealerLanding < 0) {
            DifferenceBetweenBillingRateToTheBuilderAndDealerLanding = 0;
        }
        $("#DiffBtnBRBAndDL" + Id + "").val(DifferenceBetweenBillingRateToTheBuilderAndDealerLanding.toFixed(2));

        var PriceAfterProjectDiscount = $("#PriceAftProjDis" + Id + "").val();
        var PriceAfterAddDisc = $("#PriceAftAddDis" + Id + "").val();
        var ActualDiscount = ((100 - (PriceAfterProjectDiscount / PriceAfterAddDisc) * 100) * 100 / 100 / 100);
        if (ActualDiscount < 0) {
            ActualDiscount = 0;
        }
        $("#ActDiscount" + Id + "").val(ActualDiscount.toFixed(2));
    }
