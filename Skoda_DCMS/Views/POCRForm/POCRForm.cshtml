        var PriceAfterAddDisc = $("#PriceAftAddDis" + Id + "").val();
        var ActualMargin = $("#ActMargin" + Id + "").val();
        var PriceAfterQuotationDiscount = ((PriceAfterAddDisc * ActualMargin) - PriceAfterAddDisc);
        if (PriceAfterQuotationDiscount < 0) {
            PriceAfterQuotationDiscount = 0;
        }
        $("#PriceAftQuotDis" + Id + "").val(PriceAfterQuotationDiscount.toFixed(2));
    }


    function PriceAftDealerBillAnnDis(Id) {
        var PriceAfterSchemesOrAddlExtraDisc = $("#PriceAftSchDis" + Id + "").val();
        var DealerSpecificInBillAnnual = $("#PriceAftDealerBillAnnDis" + Id + "").val();
        var PriceAfterDealerSpecificInBillAnnualDisc = ((PriceAfterSchemesOrAddlExtraDisc * DealerSpecificInBillAnnual) - PriceAfterSchemesOrAddlExtraDisc);

        if (PriceAfterDealerSpecificInBillAnnualDisc < 0) {
            PriceAfterDealerSpecificInBillAnnualDisc = 0;
        }
        $("#PriceAftProjDis" + Id + "").val(PriceAfterDealerSpecificInBillAnnualDisc.toFixed(2));

        var PriceAfterProjectDiscount = $("#PriceAftSchDis" + Id + "").val();
        var ADDGST = $("#GST" + Id + "").val();
        var ActualGSTPrice = (PriceAfterProjectDiscount * ADDGST);
        if (ActualGSTPrice < 0) {
            ActualGSTPrice = 0;
        }
        $("#ExeGSTPrice" + Id + "").val(ActualGSTPrice.toFixed(2));
    }

    function ExePriceSecFreight(Id) {
        var PriceAfterPropotinatePrimaryFreight = $("#ExePricePriFreight" + Id + "").val();
        var AddSecondaryFreight = $("#ExePriceSecFreight" + Id + "").val();
        var PriceAfterAddSecdFreight = (PriceAfterPropotinatePrimaryFreight + (PriceAfterPropotinatePrimaryFreight * AddSecondaryFreight));
        if (PriceAfterAddSecdFreight < 0) {
            PriceAfterAddSecdFreight = 0;
        }
        var AddCostOfInterestPerageforCreditIfAny = $("#ExePriceCOIForCredit" + Id + "").val();
        var PriceAfterCostOfIntPerageForCredit = (PriceAfterAddSecdFreight + (PriceAfterAddSecdFreight * AddCostOfInterestPerageforCreditIfAny));
        if (PriceAfterCostOfIntPerageForCredit < 0) {
            PriceAfterCostOfIntPerageForCredit = 0;
        }
        var ActualGSTPrice = $("#ExeGSTPrice" + Id + "").val();
        if (ActualGSTPrice == "") {
            ActualGSTPrice = 0;
        }
        $("#ExeDealerLanding" + Id + "").val(PriceAfterCostOfIntPerageForCredit + ActualGSTPrice);
    }


    function ExeDealerLanding(Id) {
        var DealerMargin = $("#DealerMargin" + Id + "").val();
        var DealerLanding = $("#ExeDealerLanding" + Id + "").val();
        var QuoteBillingRatetoBuilder = $("#CompetitorQuote" + Id + "").val();
        var DifferenceBetweenBillingRateToTheBuilderDealerLanding = $("#ExeDiffBtnBRBAndDL" + Id + "").val();
        var BillingRateToTheBuilderProjectDiscount = (DealerLanding + (DealerLanding * DifferenceBetweenBillingRateToTheBuilderDealerLanding));
        var BillingRateToTheBuilderIncludingDealerMargin = (DealerLanding + (DealerLanding * DealerMargin));
        if (BillingRateToTheBuilderIncludingDealerMargin < 0) {
            BillingRateToTheBuilderIncludingDealerMargin = 0;
        }
        $("#BillRateBuildInclDealMargin" + Id + "").val(BillingRateToTheBuilderIncludingDealerMargin.toFixed(2));

        DifferenceBetweenBillingRateToTheBuilderDealerLanding = ((BillingRateToTheBuilderIncludingDealerMargin - DealerLanding) / DealerLanding);
        var DifferenceBetweenBillingRateToTheBuilder = ((BillingRateToTheBuilderIncludingDealerMargin - QuoteBillingRatetoBuilder) / QuoteBillingRatetoBuilder);
        if (BillingRateToTheBuilderIncludingDealerMargin < 0) {
            BillingRateToTheBuilderIncludingDealerMargin = 0;
        }
        if (DifferenceBetweenBillingRateToTheBuilder < 0) {
            DifferenceBetweenBillingRateToTheBuilder = 0;
        }
        if (DifferenceBetweenBillingRateToTheBuilderDealerLanding < 0) {
            DifferenceBetweenBillingRateToTheBuilderDealerLanding = 0;
        }

        $("#DiffBtnBRBFVC" + Id + "").val(DifferenceBetweenBillingRateToTheBuilder.toFixed(2));
        $("#ExeDiffBtnBRBAndDL" + Id + "").val(DifferenceBetweenBillingRateToTheBuilderDealerLanding.toFixed(2));


        var WithoutPOCRDifference = ((BillingRateToTheBuilderProjectDiscount - QuoteBillingRatetoBuilder) / QuoteBillingRatetoBuilder);
        if (WithoutPOCRDifference < 0) {
            WithoutPOCRDifference = 0;
        }
        $("#DiffWOPOCR" + Id + "").val(DifferenceBetweenBillingRateToTheBuilder.toFixed(2));
    }

</script>

