        var PriceAfterQuotationDiscount = $("#PriceAftAddDis" + Id + "").val();
        var InBillSchemeTruckScheme = $("#PriceAftTruckSch" + Id + "").val();
        var PriceAfterSchemesOrAddlExtraDisc = ((PriceAfterQuotationDiscount * InBillSchemeTruckScheme) - PriceAfterQuotationDiscount);

        if (PriceAfterSchemesOrAddlExtraDisc < 0) {
            PriceAfterSchemesOrAddlExtraDisc = 0;
