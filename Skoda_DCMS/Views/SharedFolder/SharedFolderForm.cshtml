@model dynamic
@{
    ViewBag.Title = "Shared Folder Creation/Change Form";
    var user = (Skoda_DCMS.Models.UserData)HttpContext.Current.Session["UserData"];
    bool IsNew = true;
    bool IsEdit = true;
    dynamic FlagRedirect = null; // For Email Update
    dynamic SFF = null;
    Layout = ViewBag.Layout;
    int RowId = 0, FormId = 0, AppRowId = 0, ApprovalType = 0;
    int FormSrId = 0;
    var IsOnBehalfSAVWIPLEmployee = false;
    var IsOnBehalfOthers = false;
    if (ViewBag != null && ViewBag.IsNew != null)
    {
        IsNew = ViewBag.IsNew;
        IsEdit = ViewBag.IsEdit;
        SFF = ViewBag.Model;
        RowId = ViewBag.RowId;
        FormId = ViewBag.FormId;
        AppRowId = ViewBag.AppRowId;
        ApprovalType = ViewBag.ApprovalType != null ? (int)ViewBag.ApprovalType : 0;
        FormSrId = SFF.one[0].Id;
        IsOnBehalfSAVWIPLEmployee = SFF.one[0].RequestSubmissionFor == "OnBehalf" && SFF.one[0].OnBehalfOption == "SAVWIPLEmployee";
        IsOnBehalfOthers = SFF.one[0].RequestSubmissionFor == "OnBehalf" && SFF.one[0].OnBehalfOption == "Others";

    }
}
