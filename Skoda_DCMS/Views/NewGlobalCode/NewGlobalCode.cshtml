<script>
        var spinner = $('#loader');
        var IsNewMode = @Html.Raw(Json.Encode(IsNewMode));
        var IsEditMode = @Html.Raw(Json.Encode(IsEditMode));
        var IsViewMode = @Html.Raw(Json.Encode(IsViewMode));
        var NGCF = @Html.Raw(Json.Encode(NGCF));
        var appRowId = @Html.Raw(Json.Encode(AppRowId));
        var formID = @Html.Raw(Json.Encode(FormId));
        var approvalType =@Html.Raw(Json.Encode(ApprovalType));
        var currentUserDesgn = @Html.Raw(Json.Encode(CurrentUserDesgn));
        var currentUserLevel =@Html.Raw(Json.Encode(CurrentUserLevel));
        var glTeam = @Html.Raw(Json.Encode(glTeam));
        var controllingTeam = @Html.Raw(Json.Encode(controllingTeam));

        pageLoad();
        var errorCount = 0;
        var errors = "";
        function pageLoad() {
            $("#headingErr").addClass('d-none');
            if (IsEditMode) {
                if (NGCF.RequestType == "Profit & Loss") {
                    $("#drpGLSeries option[value='1']").hide();
                    $("#drpGLSeries option[value='2']").hide();
                    $("#drpGLSeries option[value='3']").hide();
                    $("#drpGLSeries option[value='4']").hide();

                    $("#drpGLSeries option[value='6']").show();
                    $("#drpGLSeries option[value='7']").show();
                    $("#drpGLSeries option[value='8']").show();
                    $("#drpGLSeries option[value='9']").show();
                }
                else if (NGCF.RequestType == "Balance Sheet") {
                    $("#drpGLSeries option[value='6']").hide();
                    $("#drpGLSeries option[value='7']").hide();
                    $("#drpGLSeries option[value='8']").hide();
                    $("#drpGLSeries option[value='9']").hide();

                    $("#drpGLSeries option[value='1']").show();
                    $("#drpGLSeries option[value='2']").show();
                    $("#drpGLSeries option[value='3']").show();
                    $("#drpGLSeries option[value='4']").show();
                }
            }


    }

    $("#btnSubmit").on('click', function (e) {
        $(".full-screen-container").removeClass('invisible');
        $(".full-screen-container").show();
        errors = "";
        validateSubmitter();
        validateApplicant();

        if ($('.chk_requestType:checkbox:checked').length == 0) {
            errors += "<li id='error" + errorCount + "'>" + "Request Type" + "</li>";
            $("#chkProfLoss").addClass('removeRequestType');
            $("#chkBal").addClass('removeRequestType');
            AddError("lblReqType", 'removeRequestType', errorCount);
            errorCount++;
        }
        else {
            RemoveError("lblReqType");
        }

        var txtNameGL = document.getElementById("txtNameGL").value;
        if (txtNameGL == "") {
            errors += "<li id='error" + errorCount + "'>" + "Name of GL to open" + "</li>";
            AddError("txtNameGL", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("txtNameGL");
        }

        var txtNameTranGL = document.getElementById("txtNameTranGL").value;
        if (txtNameTranGL == "") {
            errors += "<li id='error" + errorCount + "'>" + "Nature of transaction to be captured in GL" + "</li>";
            AddError("txtNameTranGL", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("txtNameTranGL");
        }

        var drpPurpose = $("#drpPurpose option:selected").val();
        if (drpPurpose == "") {
            errors += "<li id='error" + errorCount + "'>" + "Purpose" + "</li>";
            AddError("drpPurpose", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("drpPurpose");
        }

        var txtDateOpenGL = document.getElementById("txtDateOpenGL").value;
        if (txtDateOpenGL == "") {
            errors += "<li id='error" + errorCount + "'>" + "Date to Open New GL" + "</li>";
            AddError("txtDateOpenGL", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("txtDateOpenGL");
        }

        var txtGLCode = document.getElementById("txtGLCode").value;
        if (txtGLCode == "") {
            errors += "<li id='error" + errorCount + "'>" + "GL Code" + "</li>";
            AddError("txtGLCode", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("txtGLCode");
        }

        var txtGLName = document.getElementById("txtGLName").value;
        if (txtGLName == "") {
            errors += "<li id='error" + errorCount + "'>" + "GL Name" + "</li>";
            AddError("txtGLName", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("txtGLName");
        }

        var drpGLSeries = $("#drpGLSeries option:selected").val();
        if (drpGLSeries == "") {
            errors += "<li id='error" + errorCount + "'>" + "GL Series" + "</li>";
            AddError("drpGLSeries", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("drpGLSeries");
        }

        var txtBusiness = document.getElementById("txtBusinessNeed").value;
        if (txtBusiness == "") {
            errors += "<li id='error" + errorCount + "'>" + "Business justification" + "</li>";
            AddError("txtBusinessNeed", '', errorCount);
            errorCount++;
        }
        else {
            RemoveError("txtBusinessNeed");
        }

        if (errors.length) {
            $(".full-screen-container").hide();
            errors = "<ol id='errorList'>" + errors + "</ol>";
            $("#headingErr").removeClass('d-none');
            document.getElementById("add_to_me").innerHTML = errors;
            if (IsEditMode) {
                $("#myModalRequest").scrollTop(0);
            }
            else {
                $(window).scrollTop(0);
            }
            return false;
        }
        $("#headingErr").addClass('d-none');
        document.getElementById("add_to_me").innerHTML = '';
        $('#ddEmpDesignation').prop("disabled", false);
        $('#txtOnBehalfdesignation').prop("disabled", false);
        $("#pageLoader").removeClass('invisible');
        $("#btnSubmit").text('Saving');
        $("#btnSubmit").prop("disabled", true);
        $("#btnCancel").prop("disabled", true);

        var form = $('#NGCForm');
        var formData = new FormData(form[0]);
        $.ajax({
            url: '/NewGlobalCode/SaveNewGlobalCode',
            processData: false,
            contentType: false,
            type: 'POST',
            dataType: 'JSON',
            data: formData,
            enctype: 'multipart/form-data',
            success: (function (data) {
                if (data.Status == 200) {
                    $("#pageLoader").addClass('invisible');
                    if (IsNewMode) {
                        $("#btnSubmit").text('Submit');
                    }
                    else {
                        $("#btnSubmit").text('Re-Submit');
                    }
                    $("#btnSubmit").prop("disabled", false);
                    $("#btnCancel").prop("disabled", false);
                    successNGCFModal(data.Message);
                }
                else {
                    failureModal(data.Message);
                    if (IsNewMode) {
                        $("#btnSubmit").text('Submit');
                    }
                    else {
                        $("#btnSubmit").text('Re-Submit');
                    }
                    $("#pageLoader").addClass('invisible');
                    $("#btnSubmit").prop("disabled", false);
                    $("#btnCancel").prop("disabled", false);
                }
                $(".full-screen-container").hide();
            }),
            error: (function (data) {
                failureModal(data.Message);
                if (IsNewMode && IsEditMode) {
                    $("#btnSubmit").text('Submit');
                }
                else {
                    $("#btnSubmit").text('Re-Submit');
                }
                $("#pageLoader").addClass('invisible');
                $("#btnSubmit").prop("disabled", false);
                $("#btnCancel").prop("disabled", false);
                $(".full-screen-container").hide();
            })
        })
        $("#tab").val(2);

    });

    $("#btnCancel").on('click', function (e) {
        $("#headingErr").addClass('d-none');
        document.getElementById("add_to_me").innerHTML = '';
        if ($('#myModalRequest').hasClass('show'))
            $('#myModalRequest').modal('hide');
        else
            $('#myModalEdit').modal('hide');
        if (IsNewMode) {
            window.location = "../Dashboard";
        }
    });

    //Request Type
    $('input:checkbox[id="chkProfLoss"]').click(function () {
        if ($(this).prop("checked") == true) {
