    function saveResponse(text) {
        debugger;
        $(".full-screen-container").removeClass('invisible');
        $(".full-screen-container").show();
        var status = '', comment = '';
        if (window.matchMedia('(max-width: 425px)').matches) {
            comment = $("#commentMobile-" + text).val();
        } else
            comment = $("#comment-" + text).val();
        switch (text) {
            case 'approve': {
                status = 'Approved';
                if (currentUserDesgn == glTeam) {
                    var txtNewGLNo = document.getElementById("txtNewGLNo").value;
                    if (txtNewGLNo == "") {
                        AddError("txtNewGLNo", '', errorCount);
                        DisplayAlert('error', 'Error', 'Please enter New GL No.', 5000);
                        $("#collapseFive").addClass('show');
                        $(".full-screen-container").hide();
                        return;
                    }
                    else {
                        RemoveError("txtNewGLNo");
                        UpdateNGCFData(status, appRowId, comment, approvalType, text);
                    }

                }
                if (currentUserDesgn == controllingTeam) {
                    var txtCommitmentItem = document.getElementById("txtCommitmentItem").value;
                    if (txtCommitmentItem == "") {
                        AddError("txtCommitmentItem", '', errorCount);
                        DisplayAlert('error', 'Error', 'Please enter Commitment Item', 5000);
                        $("#collapseFive").addClass('show');
                        $(".full-screen-container").hide();
                        return;
                    }
                    else {
                        RemoveError("txtCommitmentItem");
                        UpdateNGCFData(status, appRowId, comment, approvalType, text);
                    }

                }
                else {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                break;
            }
            case 'reject': {
                status = 'Rejected';
                if (comment == '') {
                    validityModal(text);
                    $(".full-screen-container").hide();
                    return;
                }
                else {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                break;
            }
            case 'enquire': {
                status = 'Enquired';
                if (comment == '') {
                    validityModal(text);
                    $(".full-screen-container").hide();
                    return;
                }
                else {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                break;
            }
        }
        $("#tab").val(1);
    }

    function FormApproval(status, appRowId, comment, approvalType, text) {
        $(".full-screen-container").removeClass('invisible');
        $(".full-screen-container").show();
        spinner.show();
        $.ajax({
            url: '/List/SaveResponse',
            type: 'POST',
            data: { response: status, appRowId: appRowId, comment: comment, approvalType: approvalType },
            dataType: 'JSON',
            success: (function (data) {
                if (data.Result == 1) {
                    successNGCFModal(text);
                    spinner.hide();
                }
                else {
                    failureModal();
                    spinner.hide();
                }
                $(".full-screen-container").hide();
            }),
            error: (function (data) {
                failureModal();
                spinner.hide();
                $(".full-screen-container").hide();
            })
        })

        $("#tab").val(1);
    }

    function UpdateNGCFData(status, appRowId, comment, approvalType, text) {
        //
        var form = $("#NGCForm").serialize();
        console.log(form);
        $.ajax({
            url: '/NewGlobalCode/UpdateNGCFData',
            type: 'POST',
            data: form,
            dataType: 'JSON',
            success: (function (data) {
                if (data == 1) {
                    FormApproval(status, appRowId, comment, approvalType, text);
                }
                else {
                    failureModal();
                }
                $(".full-screen-container").hide();
            }),
            error: (function (data) {
                failureModal();
                $(".full-screen-container").hide();
            })
        })

    }

    function requiredModal(text) {
        $('#modalTitle').html('Required!');
        $('#modalBody').html(text);
        $("#modalOkButton").attr('data-dismiss', 'modal');
        $("#successModel").modal('show');
    }

    function successNGCFModal(txt) {
        ////
        $('#modalTitle').html('Success');
        if (txt == "approve") {
            $('#modalBody').html('Form has been approved!');
        }
        else if (txt == "reject") {
            $('#modalBody').html('Form has been rejected!');
        }
        else if (txt == "enquire") {
            $('#modalBody').html('Form has been enquired!');
        }
        else if (txt == "cancelled") {
            $('#modalBody').html('Form has been cancelled!');
        }
        else {
            if (IsEditMode) {
                $('#modalBody').html('Your form with ID: NGCF' + txt + ' has been re-submitted! Kindly use this for future reference');
            }
            else {
                $('#modalBody').html('Your form with ID: NGCF' + txt + ' has been submitted! Kindly use this for future reference');
            }
        }

        $("#modalOkButton").attr('data-dismiss', 'modal');
        $("#successModel").modal('show');
    }

    function failureModal(msg) {
        $('#modalTitle').html('Failure');
        $('#modalBody').html(msg);
        $("#modalOkButton").attr('data-dismiss', 'modal');
        $("#successModel").modal('show');
    }

    function permissionModal() {
        $('#modalTitle').html('Failure');
        $('#modalBody').html('No permssion granted since the device is not SAVWIPL owned!');
        $("#modalOkButton").attr('data-dismiss', 'modal');
        $("#successModel").modal('show');
    }

</script>
