                }
        }
        }

       function checkGPN(){
        var GlobalProcessNo= $("#txtGlobalProNo").val();
            if (GlobalProcessNo != "") {
                var character = "";
                var year = "";
                var number = "";
                var curryear = new Date().getFullYear();
                if (GlobalProcessNo.length != 16) {
                    alert("The Global Processs no format is not correct please write in correct format");
                    $("#txtGlobalProNo").val('');
                    return false;
                }
                character = GlobalProcessNo.substr(0, 4).toLowerCase();
                year = parseInt(GlobalProcessNo.substr(5, 4));

                if (character != "savw") {

                    alert("The Global Processs no format is not correct. The first four character should be 'SAVW' ");
                    $("#txtGlobalProNo").val('');
                    return false;
                }
                if (year > curryear) {

                    alert("The Global Processs no format is not correct. The next four character should not be greater than current year ");
                    $("#txtGlobalProNo").val('');
                    return false;
                }
            }
            else {
                alert("The Global Processs no cannot be empty!");
                $("#txtGlobalProNo").val('');
                return false;
            }

        }

        function allowAlphaNumericSpace(thisInput) {
            var arr = thisInput.value.split('');

            if (arr[0] === "." || arr[0] === "-") {
                thisInput.value = "";
            } else {
                for (var i = 0; i < arr.length; i++) {
                    if (arr[i] == "-" || arr[i] == "*" || arr[i] == "+" || arr[i] == "/") {
                        thisInput.value = "";
                    }
                }
            }

           //thisInput.value = thisInput.value.split(/[^e]/).join('');
        }


