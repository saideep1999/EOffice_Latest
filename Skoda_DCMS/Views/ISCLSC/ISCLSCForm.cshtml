<body>
    <div class="right_col bg-transparent" role="main">
        <form id="ISLSForm" method="post">
            <div class="container">
                <div>
                    @if (IsEditMode || IsViewMode)
                    {
                        <div>
                            <input type="hidden" id="FormId" name="FormId" value="@(FormId)" />
                            <input type="hidden" id="FormSrId" name="FormSrId" value="@(ISLS.Id)" />
                            <input type="hidden" id="AppRowId" name="AppRowId" value="@(AppRowId)" />
                            @*<input type="hidden" name="ApplicationCategoryDataCurr" id="ApplicationCategoryDataCurr" value="@(URCF.ApplicationCategoryData.Count)" />*@
                        </div>
                    }
                    else
                    {
                        <div class="breadcrumb-container">
                            <div>
                                <ol class="breadcrumb">
                                    <li>
                                        <a href="/Dashboard?formName=NGCF">
                                            Dashboard
                                        </a>
                                    </li>
                                    <li>
                                        ISC LST Form
                                    </li>
                                </ol>
                            </div>
                        </div>
                    }
                </div>

                <div class="row breadcrumb-container table-bordered  mr-1 ml-1" style="background-color: rgb(219,238,213) !important; ">
                    <div class="col-md-2 col-lg-2">
                        <span class="placeholder">Date : @(IsViewMode || IsEditMode ? (Convert.ToDateTime(ISLS.FormIDISLS.CreatedDate).ToShortDateString()) : DateTime.Now.ToShortDateString())</span>
                    </div>
                    <div class="col-md-8 col-lg-8 mt-2 mb-1 form-heading" style="text-align:center; margin-top:5px">
                        <span>ISC LST Form</span>
                    </div>
                    <div class="col-md-2 col-lg-2">
                        @if (IsViewMode || IsEditMode)
                        {
                            <span class="placeholder white_space_nowrap"> Form Id : @(IsViewMode || IsEditMode ? ("ISLS" + FormId ?? "") : "" )</span>
                        }
                    </div>
                </div>

            </div>

            <div class="heading d-none" id="headingErr">
                <h1 id="" class="info-header">Following fields are mandatory. Please fill in information for<br></h1>
                <div id="add_to_me" class="header-list" style="color:red;"></div>
            </div>
            <!-- ACCORDIAN STRUCTURE START HERE-->
            @* Submitter Section *@
            @{
                Html.RenderPartial("_NewSubmitterDetailsWithModel", (ViewBag as object));
            }

            @* Applicant Section *@
            @{
                Html.RenderPartial("_NewApplicantDetailsWithModel", (ViewBag as object));
            }

            <div id="accordion">
                <div class="card">

                    <div class="card-header collapsed" id="headingThree" data-toggle="collapse" data-target="#collapseThree" aria-expanded="true" aria-controls="collapseThree">
                        <h5 class="mb-0">
                            <button type="button" class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseThree" aria-expanded="true" aria-controls="collapseThree">
                                <img src="~/assets/img/transactionIcon.svg" width="24" height="24" class="mr-2" /><span class="d-inline-block mt-1">Transaction Fields</span>
                            </button>
                        </h5>
                    </div>
                    <div id="collapseThree" class="collapse show" aria-labelledby="headingThree" style="">
                        @if (IsNewMode)
                        {
                            <div class="card-body">

                                <div class="row">
                                    <div class="col-12">
                                        <table class="table mainTbl form-table-container-2">
                                            <tbody>
                                                <tr>
                                                    <td class="actionTypeWidth"><label id="lblActionType" class="f-600">Action Type<span class="astColor">*</span></label></td>
                                                    <td class="w18">
                                                        <label class="checkbox-container f-600">
                                                            ISC
                                                            <input type="radio" class="inpDisable" name="chkISCLST" id="chkISC" value="ISC" onclick="getActionType()" checked="checked">
                                                            <span class="checkmark"></span>
                                                        </label>
                                                    </td>
                                                    <td class="align w-20">
                                                        <label class="checkbox-container f-600">
                                                            LST
                                                            <input type="radio" class="inpDisable" name="chkISCLST" id="chkLST" value="LST" onclick="getActionType()">

                                                            <span class="checkmark"></span>
                                                        </label>
                                                    </td>
                                                    <td>
                                                        <input type="hidden" name="ActionType" id="chkActionType" value="">
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>


                                <div class="row">
                                    <div class="form-group col-md-3 col-lg-3 col-12 input-block">

                                        <input type="text" class="w-100 txtDisable" name="BuyerName" id="txtBuyerName" value="" required="" readonly>
                                        <span class="placeholder">Buyer Name<span class="astColor">*</span></span>

                                    </div>
                                    <div class="col-md-3 col-lg-3 col-12 inputSelect">
                                        <select name="Team" class="" id="drpTeam" onchange="checkSelect('drpTeam')" required>
                                            <option style="display: none"></option>
                                            <option value="BA/1">BA/1</option>
                                            <option value="BA/2">BA/2</option>
                                            <option value="BA/3">BA/3</option>

                                        </select>
                                        <span class="placeholder TransView">Team<span class="astColor">*</span></span>
                                    </div>
                                    <div class="form-group col-md-3 col-lg-3 col-12 input-block">

                                        <input type="text" class="w-100 txtDisable" name="GlobalProcessNumber" id="txtGlobalProNo" value="" required="">
                                        <span class="placeholder">Global Process Number<span class="astColor">*</span></span>

                                    </div>
                                    <div class="form-group col-md-3 col-lg-3 col-12 input-block">

                                        <input type="text" class="w-100 txtDisable" name="Description" id="txtDesc" value="" required="">
                                        <span class="placeholder">Description<span class="astColor">*</span></span>

                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-md-3 col-lg-3 col-12 input-block">

                                        <input type="number" min="0" class="w-100 txtDisable spanUpperSide" name="InitialBudget" id="txtBudget" value="" required="" oninput="allowAlphaNumericSpace(this)">
                                        <span class="placeholder">Budget (in Euros)<span class="astColor">*</span></span>

                                    </div>
                                    <div class="col-md-3 col-lg-3 col-12 inputSelect">
                                        <select name="Status1" class="" id="drpstatus1" onchange="checkSelect('drpstatus1')" required>
                                            <option style="display: none"></option>
                                            <option class="StatusIST" value="For bidder list approval">For bidder list approval</option>
                                            <option class="StatusLST" value="ForPresentation">For Presentation</option>

                                        </select>
                                        <span class="placeholder TransView">Status1<span class="astColor">*</span></span>
                                    </div>


                                    <div class="form-group col-md-3 col-lg-3 col-12 input-block">

                                        <input type="date" class="w-40" name="BidderApprovalDate" id="txtBidAppDate" required="">
                                        <span class="placeholder" style="background-color: #fff !important;">Bidder Approval Date<span class="astColor">*</span></span>

                                    </div>

                                    <div class="form-group col-md-3 col-lg-3 col-12 input-block">
