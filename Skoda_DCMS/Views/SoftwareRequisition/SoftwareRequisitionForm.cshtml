            $('#SAVWIPLEmployeeDetailsSection').addClass('d-none');
            $('#OthersEmployeeDetailsSection').addClass('d-none');
            $('input:radio[value="SAVWIPLEmployee"]').prop("checked", false).val();
            $('input:radio[value="Others"]').prop("checked", false).val();

        }
