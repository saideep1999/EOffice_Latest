                                else {
                                    if (!($(this).text()) == '(sign)')
                                        tempObject.SubAreas.push($(this).text());
                                }

                            });
                        });
                    });
                    if (area != undefined) {
                        abc.push(tempObject);
                    }
                });

                //console.log(abc);

