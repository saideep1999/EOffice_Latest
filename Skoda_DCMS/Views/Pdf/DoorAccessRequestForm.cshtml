                //console.log(LocationName);

                var form = $("#DARForm").serialize();
                form += "&areaArr=" + JSON.stringify(abc);

                //console.log(form);
            }
        }
        $("#pageLoader").removeClass('invisible');
        //return;

        $.ajax({
            url: '/IDCard/SaveIDCard',
            type: 'POST',
            data: form,
            dataType: 'JSON',
