    function date_val() {
        var week_string = document.getElementsByName("txtTempFrom")[0].value;
        var parts = week_string.split("-");
        var date = new Date(parts[0], parts[1] - 1, parts[2]);

        if (isValidDate(date)) {
            var min_date = date;
            min_date.setDate(min_date.getDate() + 1);
            var initial_date = min_date.toISOString().split('T')[0];
            console.log(initial_date);
            document.getElementsByName("txtTempTo")[0].setAttribute("min", initial_date);
        }
    }

    function isValidDate(d) {
        return d instanceof Date && !isNaN(d);
    }

