
    <div style="align-items:center; margin-left:100px">
        <label for="tags">Tags: </label>
        <input id="tags" onclick="autocompleteInput();">
    </div>
    <script>
        function autocompleteInput() {
            $("#tags").autocomplete({
                minLength: 2,
                source: function (request, response) {
                    $.ajax({
                        url: "/Dashboard/SearchForms",
                        type: "POST",
                        dataType: "json",
                        data: { Prefix: request.term },
                        success: function (data) {
                            console.log(data);
                            let result = data.map(a => a.FormName);
                            response(result);
                        }
                    });
                },
                select: function (event, ui) {
                    alert("Selected: ");
                }
            });
        }
        
    </script>
