    $('#softwareBody').on('click', '.remove', function () {
        //var rows = parseInt($('#UserIdTableRowCount').val());
        var rows = SrNo;
        var id = $(this).attr('id');

        var deleteRowNum = parseInt(id.split("_")[1]);
        $('#row_' + deleteRowNum).remove();
        var index = deleteRowNum;
        //Below code will normalise the row number and the ids of the input
        $('#softwareBody').children().each(function () {
            var trId = $(this).attr('id');
            var rowNum = parseInt(trId.replace("row_", ""));
            if (rowNum > deleteRowNum) {
                var selector = "#" + trId + " input[type='text'], " + "#" + trId + " textarea, " + "#" + trId + " select, " + "#" + trId + " input[type='image']";
                $(selector).each(function (i, x) {
                    x.id = x.id.replace(/_.*/, "_" + index);
                });
                $("#" + trId).find('.sNo').html('' + index);
                $(this).attr('id', 'row_' + index);
                index++;
            }
        });
        --rows;
        $('#UserIdTableRowCount').val(rows);
    });





