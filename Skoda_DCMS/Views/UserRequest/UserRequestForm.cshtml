            a += '<option value="' + serviceCategoryList[i] + '">' + serviceCategoryList[i] + '</option>';
        }

        for (var i = 0; i < serviceSubCategoryList.length; i++) {
            b += '<option value="' + serviceSubCategoryList[i] + '">' + serviceSubCategoryList[i] + '</option>';
        }

        for (var i = 0; i < allData.length; i++) {
            c += '<option value="' + allData[i].Role + '">' + allData[i].Role + '</option>';
        }

        for (var j = 0; j < len; j++) {

            $('#drpServiceCategory' + (j + 1)).html(a);
            $("#drpServiceCategory" + (j + 1) + " option[value='" + value_1[j].ServiceCategory + "']").prop('selected', 'selected');

            $('#drpServiceSubCategory' + (j + 1)).html(b);
            $("#drpServiceSubCategory" + (j + 1) + " option[value='" + value_1[j].ServiceSubCategory + "']").prop('selected', 'selected');

            if (value_1[j].Role !="Select Role") {
                $('#drpRole' + (j + 1)).html(c);
                $("#drpRole" + (j + 1) + " option[value='" + value_1[j].Role + "']").prop('selected', 'selected');
            }

        }
    }

