    function getServiceSubCategoryByServiceCategory(locationElem, deptElemId, serviceCategory = '', selectedDept = '') {
        //
        $('#' + deptElemId).prop("disabled", false);
        let deptList = [];
        let distinctDept = [];
        var locationElemId = locationElem.id;
        if (serviceCategory == '') {
            var selectedLoctxt = $('#' + locationElemId).find("option:selected").text();
            console.log("selectedLoctxt=" + selectedLoctxt);
            serviceCategory = selectedLoctxt;
        }
        deptList = allData.filter(function (item) { return (item.ServiceCategory == serviceCategory); });
        distinctDept = [...new Set(deptList.map(item => item.ServiceSubCategory))];
        //console.log("distinctDept=" + distinctDept);
        let s = '<option value="-1" style="display:none;" selected>Select Service Sub Category</option>';
        for (var i = 0; i < distinctDept.length; i++) {
            s += '<option value="' + distinctDept[i] + '">' + distinctDept[i] + '</option>';
        }
        $('#' + deptElemId).html(s);


    }

    function getRoleByServiceSubCategory(deptElem, locationElemId, doorElemId, serviceCategory = '', ServiceSubCategory = '') {
        //

        $('#' + doorElemId).prop("disabled", false);
        let deptId = deptElem.id;
        if (serviceCategory == '') {
            var selectedLoctxt = $('#' + locationElemId).find("option:selected").text();
            //console.log("selectedLoctxt=" + selectedLoctxt);
            serviceCategory = selectedLoctxt;
        }
        if (ServiceSubCategory == '') {
            var selectedDepttxt = $('#' + deptId).find("option:selected").text();
            //console.log("selectedDepttxt=" + selectedDepttxt);
            ServiceSubCategory = selectedDepttxt;
        }
        let roleList = [];

        roleList = allData.filter(function (item) { return (item.ServiceCategory == serviceCategory && item.ServiceSubCategory == ServiceSubCategory); });
        let s = '';
        if (serviceCategory == 'HOAS') {
            s = '<option value="-1" style="display:none;" selected>Select Role Name</option>';
            for (var i = 0; i < roleList.length; i++) {
                s += '<option value="' + roleList[i].Role + '">' + roleList[i].Role + '</option>';
            }
            $('#' + doorElemId).html(s);
        }
