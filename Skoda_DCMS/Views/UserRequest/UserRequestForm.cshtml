            + '<td><input type="text" class="inpApplicationUserIdValidate" autocomplete="off" name="ApplicationUserId" id = "txtApplicationUserId' + newCount + '"></input></td>'
            + '<td style="background-color:white; max-width:70px; vertical-align:central;">'
            + '<input class="remove-btn remove" type="image" src="/assets/img/remove.png" alt="Remove" style="max-width: 85%; background-color: white;">'
            + '</td></tr>';

        if (newCount == 1) {
            $('.tableApplicationCategory').prepend(row);
        }
        else {
            $(".tableApplicationCategory tr:last").after(row);
        }

        getServiceCategory("drpServiceCategory" + newCount);
        getAccessType("drpAccessType" + newCount);
        getBrand("drpBrand" + newCount);
