<script>
    $("#nav-mytask-tab").on('click', function () {
        window.location = "../Dashboard";
    });
    $('#nav-mytask-tab').on('click', function () {
        GetMyTaskTab();
        DisplayMyTaskTab();
    });
    $('#nav-myrequest-tab').on('click', function () {
        //DisplayMyRequestTab();
        ViewMyRequest();
        GetMyRequestTab();
    });
    $('#nav-myReport-tab').on('click', function () {
        $(".nav-upper-options .TabActive").removeClass("TabActive");
        $(".nav-upper-options .option3").addClass("TabActive");
        GetMyReportTab();
    });
    function GetMyReportTab() {
        ViewMyReport();

        $("#loaderMyReq").removeClass('d-none');
        $.ajax({
            url: '/Dashboard/GetMyReport',
            type: 'GET',
            dataType: 'html',
            data: { status: "Report" },
            success: (function (data) {
                /*    $('#requestTable').dataTable().fnDestroy();*/
                //console.log(data);
                $("#nav-myReport").html(data);
                $("#loaderMyReq").addClass('d-none');
            }),
            error: (function (data) {
                console.log("There was an issue while refreshing tasks. Please reload the page");
            })
        })
    }
    function GetMyRequestTab() {

        $("#loaderMyReq").removeClass('d-none');
        $.ajax({
            url: '/Dashboard/GetMyReqtab',
            type: 'GET',
            dataType: 'html',
            success: (function (data) {
                /*    $('#requestTable').dataTable().fnDestroy();*/
                //console.log(data);
                $("#nav-myrequest").html(data);
                $("#loaderMyReq").addClass('d-none');
            }),
            error: (function (data) {
                console.log("There was an issue while refreshing tasks. Please reload the page");
            })
        })
    }

    function GetMyTaskTab() {
        ViewDashboard();
        /*alert('my task load');*/
        $("#loaderMytsk").removeClass('d-none');
        $.ajax({
            url: '/Dashboard/GetMyTasks',
            type: 'GET',
            dataType: 'html',
            success: (function (data) {
                console.log(data);
                $("#loaderMytsk").addClass('d-none');
                $("#myTaskView").html(data);
                $('#taskTable').dataTable().fnDestroy();
                MyTaskTable();
            }),
            error: (function (data) {
                console.log("There was an issue while refreshing tasks. Please reload the page");
                $("#loaderMytsk").addClass('d-none');
            })
        })
    }
    function ViewDashboard() {
        $(".dvdashboard").removeClass("d-none");
        $("#dvMyRequest").addClass("d-none");
        $("#dvMyReport").addClass("d-none");
        $(".nav-upper-options .TabActive").removeClass("TabActive");
        $(".nav-upper-options .option1").addClass("TabActive");
    };
    function ViewMyRequest() {
        $(".dvdashboard").addClass("d-none");
        $("#dvMyRequest").removeClass("d-none");
        $("#dvMyReport").addClass("d-none");
        $(".nav-upper-options .TabActive").removeClass("TabActive");
        $(".nav-upper-options .option2").addClass("TabActive");
    };
