            })
        })
    }

    function validateEmail(emailId, domainIdList) {
        ////
        var validRegex = null;
        var EmailValidate = '@System.Configuration.ConfigurationManager.AppSettings["EmailValidate"]';
        console.log("EmailValidate=" + EmailValidate);
            var arrEmails = [];

        if (EmailValidate == "mailnesia") {
            arrEmails = domainIdList;
        }
        else if (EmailValidate == "skoda") {
            arrEmails = domainIdList;
        }
        if (EmailValidate == "mailnesia") {
            validRegex = [];
            arrEmails.forEach((x) => validRegex.push(new RegExp("^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@@" + x + "$", "i")));
        }
        else if (EmailValidate == "skoda") {
            validRegex = [];
            arrEmails.forEach((x) => validRegex.push(new RegExp("^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@@" + x + "$", "i")));
        }

        var receivedEmail = emailId;
        console.log(receivedEmail);
        var valid = validRegex.filter(x => {
            if (!(receivedEmail.match(x))) {
                return false;
            }
            else
                return true;
        });
        return valid.length > 0;
    }
